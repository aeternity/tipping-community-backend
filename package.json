    "bignumber.js": "^9.0.1",
    "cld": "^2.7.0",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^5.3.4",
    "express-openapi-validator": "^4.13.1",
    "fuse.js": "^6.4.6",
    "glob": "^7.2.0",
    "ipfs-http-client": "^56.0.0",
    "languagedetect": "^2.0.0",
    "metascraper": "^5.24.8",
    "metascraper-description": "^5.24.8",
    "metascraper-image": "^5.24.8",
