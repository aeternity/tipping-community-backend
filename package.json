    "url": "https://github.com/aeternity/tipping-community-backend/issues"
  },
  "homepage": "https://github.com/aeternity/tipping-community-backend#readme",
  "dependencies": {
    "@aeternity/aepp-sdk": "^9.0.1",
    "@sentry/node": "^6.13.2",
    "@sentry/tracing": "^6.13.2",
    "aeternity-fungible-token": "git+https://git@github.com/aeternity/aeternity-fungible-token#v2.0.0",
    "async-lock": "^1.3.0",
