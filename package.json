    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "mocha": "^10.4.0",
    "rewire": "^7.0.0",
    "sequelize-auto-migrations": "git+https://git@github.com/scimonster/sequelize-auto-migrations#a063aa6535a3f580623581bf866cef2d609531ba",
