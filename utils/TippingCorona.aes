payable contract WaelletTipAnyBasic =
  record state =
    { tips          : map(string * int, tip)
    , tips_flat     : map(string, int)
    , pay_for_tx_service : address }

  record tip =
    { sender        : address
    , received_at   : int
    , repaid        : bool
    , amount        : int
    , note          : option(string) }

  entrypoint get_state() : state = state
